{
  "name": "kiosk-agent",
  "version": "1.0.0-dev",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "npm run build --workspace=@selfservice/obd-diagnostics",
    "dev": "node --loader ts-node/esm --no-warnings src/main.ts",
    "build": "tsc -p tsconfig.json && node scripts/copy-public.cjs",
    "start": "node dist/main.js",
    "test": "node --loader ts-node/esm --test ./src/**/*.test.ts",
    "typecheck:strict": "tsc --noEmit --strict",
    "self-check:obd": "node --loader ts-node/esm src/devices/obd/runSelfCheck.ts",
    "smoke:obd": "node --loader ts-node/esm --no-warnings src/smoke-tests/obd-smoke.ts",
    "smoke:thickness": "node --loader ts-node/esm --no-warnings src/smoke-tests/thickness-smoke.ts",
    "smoke:payments": "node --loader ts-node/esm --no-warnings src/smoke-tests/payments-smoke.ts",
    "smoke:all": "node --loader ts-node/esm --no-warnings src/smoke-tests/run-all.ts",
    "dtc:import": "node --loader ts-node/esm src/dtc/runImport.ts",
    "update-agent": "node dist/update-agent/main.js",
    "update-agent:dev": "node --loader ts-node/esm --no-warnings src/update-agent/main.ts",
    "update-agent:install": "node dist/update-agent/installer/windows-service.js install",
    "update-agent:uninstall": "node dist/update-agent/installer/windows-service.js uninstall",
    "update-agent:start": "node dist/update-agent/installer/windows-service.js start",
    "update-agent:stop": "node dist/update-agent/installer/windows-service.js stop",
    "update-agent:restart": "node dist/update-agent/installer/windows-service.js restart"
  },
  "dependencies": {
    "@selfservice/obd-diagnostics": "workspace:*",
    "@abandonware/noble": "^1.9.2-15",
    "@supabase/supabase-js": "^2.45.0",
    "@types/form-data": "^2.2.1",
    "@types/node-cron": "^3.0.11",
    "@types/ws": "^8.18.1",
    "adm-zip": "^0.5.10",
    "async-mutex": "^0.5.0",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "duckdb": "^1.1.3",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "noble-winrt": "^0.1.1",
    "node-cron": "^4.2.1",
    "node-windows": "^1.0.0-beta.8",
    "nodemailer": "^6.10.1",
    "obd-parser": "^0.3.0",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.5",
    "serialport": "^12.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.5",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.12.12",
    "@types/node-windows": "^0.1.6",
    "@types/nodemailer": "^6.4.20",
    "@types/pg": "^8.15.5",
    "@types/uuid": "^9.0.8",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
